<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.auleSVGWT.server.domain.Building" table="building">
        <id name="number" column="building_number">
            <generator class="native"/>
        </id>
        <property name="name" column="building_name"/>


    </class>
</hibernate-mapping>