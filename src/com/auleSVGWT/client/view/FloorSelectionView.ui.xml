<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my="urn:import:com.auleSVGWT.client.common">
    <ui:style field="otherStyle" src="View.css"></ui:style>
    <g:HTMLPanel>
        <g:HorizontalPanel styleName="{otherStyle.header}">
            <g:VerticalPanel  styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Edificio</g:Label>
                <my:MyListBox ui:field="buildingLst" styleName="{otherStyle.headerFontWidget}" ></my:MyListBox>
            </g:VerticalPanel>
            <g:VerticalPanel styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Piano</g:Label>
                <my:MyListBox ui:field="floorLst" styleName="{otherStyle.headerFontWidget}" ></my:MyListBox>
            </g:VerticalPanel>
            <g:VerticalPanel styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Tipo Visualizzazione</g:Label>
                <g:HorizontalPanel>
                    <my:MyListBox ui:field="mapLst" styleName="{otherStyle.headerFontWidget}" ></my:MyListBox>
                    <g:Button ui:field="enterBtn" styleName="{otherStyle.headerFontWidgetMarginLeft}" >Enter</g:Button>

                </g:HorizontalPanel>


            </g:VerticalPanel>


            <g:VerticalPanel styleName="{otherStyle.header}" >
                <g:Label styleName="{otherStyle.header2}">Inserisci Nome</g:Label>
                <g:TextBox ui:field="txtName" styleName="{otherStyle.headerFontWidget}" ></g:TextBox>



            </g:VerticalPanel>
            <g:VerticalPanel styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Cognome:</g:Label>
                <g:HorizontalPanel>
                    <g:TextBox ui:field="txtSurname" styleName="{otherStyle.headerFontWidget}"></g:TextBox>
                    <g:Button ui:field="searchPersonButton" styleName="{otherStyle.headerFontWidgetMarginLeft}">Cerca</g:Button>


                </g:HorizontalPanel>



            </g:VerticalPanel>

        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>