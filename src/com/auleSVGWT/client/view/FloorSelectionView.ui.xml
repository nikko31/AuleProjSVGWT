<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my="urn:import:com.auleSVGWT.client.common">
    <ui:style field="otherStyle" src="View.css"></ui:style>
    <g:HTMLPanel>
        <g:HorizontalPanel styleName="{otherStyle.header}">
            <g:VerticalPanel  styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Edificio</g:Label>
                <my:MyListBox ui:field="buildingLst"></my:MyListBox>
            </g:VerticalPanel>
            <g:VerticalPanel styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Piano</g:Label>
                <my:MyListBox ui:field="floorLst"></my:MyListBox>
            </g:VerticalPanel>
            <g:VerticalPanel styleName="{otherStyle.header1}">
                <g:Label styleName="{otherStyle.header2}">Tipo Visualizzazione</g:Label>
            <my:MyListBox ui:field="mapLst"></my:MyListBox>
            </g:VerticalPanel>
            <g:Button ui:field="enterBtn" styleName='{otherStyle.enterButton1}'>Enter</g:Button>

        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>