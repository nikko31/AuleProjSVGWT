<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:my="urn:import:com.auleSVGWT.client.common"
        xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">
    <g:HTMLPanel>
        <table>
            <tr>
                <td>Nome:</td>
                <td>
                    <g:TextBox ui:field="firstNameTxt"/>
                </td>
            </tr>
            <tr>
                <td>Cognome:</td>
                <td>
                    <g:TextBox ui:field="lastNameTxt"/>
                </td>
            </tr>
            <tr>
                <td>Email:</td>
                <td>
                    <g:TextBox ui:field="emailTxt"/>
                </td>
            </tr>
            <tr>
                <td>Numero di telefono:</td>
                <td>
                    <g:TextBox ui:field="phoneTxt"/>
                </td>
            </tr>
            <tr>
                <td>Data di inizio contratto:</td>
                <td>
                    <dp:DateBox ui:field="startWTxt"/>
                </td>
            </tr>
            <tr>
                <td>Data di fine contratto:</td>
                <td>
                    <dp:DateBox ui:field="endWTxt"/>
                </td>
            </tr>
            <tr>
                <td>Ruolo:</td>
                <td>
                    <my:RoleDTOListBox ui:field="rolesList"/>
                </td>
            </tr>
        </table>
        <br/>

        <table>
            <tr>
                <td><g:Button ui:field="saveButton">Salva</g:Button></td>
                <td><g:Button ui:field="cancelButton">Indietro</g:Button></td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>