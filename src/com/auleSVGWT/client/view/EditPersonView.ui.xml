<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:my="urn:import:com.auleSVGWT.client.common">
    <g:HTMLPanel>
        <table>
            <tr>
                <td>First name:</td>
                <td>
                    <g:TextBox ui:field="firstNameTxt"/>
                </td>
            </tr>
            <tr>
                <td>Last name:</td>
                <td>
                    <g:TextBox ui:field="lastNameTxt"/>
                </td>
            </tr>
            <tr>
                <td>Email:</td>
                <td>
                    <g:TextBox ui:field="emailTxt"/>
                </td>
            </tr>
            <tr>
                <td>Role:</td>
                <td>
                    <my:RoleDTOListBox ui:field="rolesList"/>
                </td>
            </tr>
        </table>
        <br/>
        <g:Button ui:field="saveButton">Salva</g:Button>
        <g:Button ui:field="cancelButton">Indietro</g:Button>
    </g:HTMLPanel>
</ui:UiBinder>