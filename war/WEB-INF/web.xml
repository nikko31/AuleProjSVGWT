<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>com.auleSVGWT.AuleSVGWT AuleSVGWTService</servlet-name>
        <servlet-class>com.auleSVGWT.server.AuleSVGWTServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.auleSVGWT.AuleSVGWT AuleSVGWTService</servlet-name>
        <url-pattern>/AuleSVGWT/AuleSVGWTService</url-pattern>
    </servlet-mapping>




    <servlet>
        <servlet-name>image</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidImageServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>image</servlet-name>
        <url-pattern>/Andro/immagine/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>people</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidPeopleServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>people</servlet-name>
        <url-pattern>/Andro/persone/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rooms</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidRoomServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>rooms</servlet-name>
        <url-pattern>/Andro/stanze/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>floorRoomlist</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidFloorRoomsListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>floorRoomlist</servlet-name>
        <url-pattern>/Andro/listaStanzePiano/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>buildingList</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidBuildingsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>buildingList</servlet-name>
        <url-pattern>/Andro/listaEdifici.json</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>beginning</servlet-name>
        <servlet-class>com.auleSVGWT.server.AndroidStart</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>beginning</servlet-name>
        <url-pattern>/Andro</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Jersey Web Application</servlet-name>
        <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
        <init-param>
            <param-name>javax.ws.rs.Application</param-name>
            <param-value>com.auleSVGWT.server.JaxApplication</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Jersey Web Application</servlet-name>
        <url-pattern>/Android/*</url-pattern>
    </servlet-mapping>

</web-app>
